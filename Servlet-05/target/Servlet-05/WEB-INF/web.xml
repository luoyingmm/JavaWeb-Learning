<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <error-page>
    <error-code>500</error-code>
    <location>/error/500.jsp</location>
  </error-page>

  <error-page>
    <error-code>404</error-code>
    <location>/error/404.jsp</location>
  </error-page>

  <servlet>
    <servlet-name>CookieTest</servlet-name>
    <servlet-class>com.luoyingmm.servlet.CookieTest</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CookieTest</servlet-name>
    <url-pattern>/ck</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>CookieTest02</servlet-name>
    <servlet-class>com.luoyingmm.servlet.CookieTest02</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CookieTest02</servlet-name>
    <url-pattern>/re</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>SessionTest</servlet-name>
    <servlet-class>com.luoyingmm.servlet.SessionTest</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SessionTest</servlet-name>
    <url-pattern>/session</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>SessionTest02</servlet-name>
    <servlet-class>com.luoyingmm.servlet.SessionTest02</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SessionTest02</servlet-name>
    <url-pattern>/session2</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>SessionTest03</servlet-name>
    <servlet-class>com.luoyingmm.servlet.SessionTest03</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SessionTest03</servlet-name>
    <url-pattern>/session3</url-pattern>
  </servlet-mapping>

  <session-config>
    <session-timeout>1</session-timeout>
  </session-config>

</web-app>
